server {
  deny  all;
  allow 192.168.1.0/24;
  allow 10.1.1.0/16;
  allow 172.27.0.0/24;
  allow IP_DATA;

  server_tokens off;
  location / {
    proxy_pass                         http://gitbook:4000;
    proxy_http_version                 1.1;
    proxy_cache_bypass                 $http_upgrade;

    # Proxy timeouts
    proxy_connect_timeout              60s;
    proxy_send_timeout                 60s;
    proxy_read_timeout                 60s;
  }
}